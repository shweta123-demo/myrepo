pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                git 'https://github.com/shweta123-demo/myrepo.git'
            }
        }

        stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }

        stage('Release') {
            steps {
                archiveArtifacts artifacts: 'target/*.jar'
            }
        }
    }
}
